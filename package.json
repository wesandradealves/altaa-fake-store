  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "next start -p 3000",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "test:unit": "NODE_ENV=test jest --passWithNoTests",
    "test:e2e": "next build && playwright test",
    "test:e2e:ui": "next build && playwright test --ui",
    "test:pwa": "next build && node -e \"require('fs').mkdirSync('.lighthouse',{recursive:true})\" && start-server-and-test \"npm run start:prod\" http://localhost:3000 \"lighthouse http://localhost:3000 --only-categories=performance,accessibility,best-practices,seo --output=html --output-path=.lighthouse/lighthouse.html --chrome-flags='--headless=new'\"",
    "test:ci": "npm run lint && npm run test:unit && npm run test:e2e && npm run test:pwa",
    "vercel-build": "npm run lint && npm run test:unit && next build",
    "lint:watch": "chokidar '**/*.{js,ts,jsx,tsx}' -c 'eslint --fix --no-warn-ignored {path}'",
    "prepare": "husky && husky install"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": "eslint --fix"
  },
